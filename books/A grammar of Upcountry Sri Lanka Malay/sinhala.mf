z3'r=z0'; z4'r=z4''l=z0l; z3''l=z0''l;
z3 = (z3''--z4'') intersectionpoint (z3'--z4');
bot z4=(4.5FX,0); draw z4{right}..z3{dir40}; pickup nullpen;
pos12(shthin,90); pos13(shfat,180); pos14(shthin,-90); z12=z2; z14=z4;
z13r=(0,y0''l); penstroke z12e{left}...z13e{down}...z14e{right};
path p; p=z13{down}...z14{right}; 
z15''=(.4FX,0); z15'=(x4,y0l); z15= (z15''--z15') intersectionpoint p;
pos15(shthin,angle(z15'-z15'')-90); pos16(shthin,-90); z16=(x4,3.7FY);
pos17(.33shvar,-90); z17r=(round(.5w)+1,0);
penstroke z15e{z15'-z15''}...z16e{right}..z17e{right};
addto currentpicture also currentpicture reflectedabout 
((round(.5w),0),(round(.5w),h));
endchar;

