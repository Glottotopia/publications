z100=(11.05FX,14FY); z102-z100=whatever*dir157.5; x102=0;
path pp; pp=z5{dir(138-90)}..z6{dir66.6}; z103=pp intersectionpoint (z100--z102);
z101-z103=z103-z100; 
make_small_circle(100);
make_small_circle(101); vowel:=false;
endchar;

