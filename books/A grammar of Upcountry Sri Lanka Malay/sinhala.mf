z1l=(16.5FX,.4FY); z2l=(7.9FX,-5.8FY); z3l=(0,.55FY); z4=(6.5FX,6.7FY);
z20=(10.8FX,y4); pos21(.5shvar,-116); z21l=(14FX,y20l);
penstroke z1e{down}...z2e{left}...z3e{up}...z4e..z21e;
pos21'(.5shvar,-116+180); z21'=z21; 
pickup shthin_pen; drawdot z1; 
z31r=(9.6FX-FX,10.7FY+.5FY); 
pos31(.277shvar,125);
z32=(6.5FX-FX,8.8FY+.5FY); z33=(x32+.777shvar,y32-(.555shvar)); 
