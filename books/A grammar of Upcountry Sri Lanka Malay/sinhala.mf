xbeginchar(oct"021",14.8FX#,18.9FY#,0); "i";
adjust_fit(usual_left#-2FX#,usual_right#); h:=12.3FY;
z0=(6.4FX,6.7FY); pos4(shfat,100); y4l=0; x4=x0; 
make_small_circle(0); z3l=(12.8FX,5FY); 
pos3(whatever,angle(z0-z3l)); x3r-x3l=-shthin; z1'= directionpoint
dir(30) of small_circle; draw z1'{dir30}...z3{down}; pickup nullpen;
z5l=(0,5.7FY); x5r-x5l=shthin; pos5(whatever,angle((x0,10FY)-z5l));
pos6(shthin,-90); z6l=(x0,12.3FX); pickup shthin_pen; rt x7=14.8FX;
y7=y0+FX; path pp; pp= z6{right}...z7; z0''-(x0,0)=whatever*dir74; y0''=20FY;
numeric tt,tt'; (tt,tt')= ((x0,0)--z0'') intersectiontimes pp;
draw subpath (0,tt') of pp;  
pickup nullpen;
penstroke z3e{down}...z4e{left}...z5e{up}...z6e{right}; 
pos10(shthin,90); pos11(1shvar,0); pos12(shthin,-90); pos13(.4shvar,180);
z10=z6; z12=(x10,y10+4.4FY); z11=.5[z10,z12]-(2.75FX,0); 
z13=.5[z10,z12]+(2.75FX,0); penstroke z10e{left}...z11e{up}...z12e{right}...
z13e{down}; fill z13l..z13r..cycle;
currentpicture:=currentpicture reflectedabout 
((0,round(6.15FY)),(30FX,round(6.15FY)));
currentpicture:=currentpicture reflectedabout 
((round(7.4FX),0),(round(7.4FX),30FY));
endchar;

xbeginchar(oct"022",12.3FX#,18.9FY#,0); "\=\i";
